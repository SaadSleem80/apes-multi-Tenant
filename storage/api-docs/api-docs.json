    "components": {
        "schemas": {
            "LoginRequest": {
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "maxLength": 255
                    },
                    "password": {
                        "type": "string",
                        "minLength": 8
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "maxLength": 255
                    },
                    "password": {
                        "type": "string",
                        "minLength": 8
                    },
                    "password_confirmation": {
                        "type": "string",
                        "minLength": 8
                    }
                },
                "type": "object"
            },
            "BookingRequest": {
                "required": [
                    "team_id",
                    "start_time",
                    "end_time",
                    "day"
                ],
                "properties": {
                    "team_id": {
                        "type": "integer",
                        "example": 1
                    },
                    "start_time": {
                        "type": "string",
                        "format": "time",
                        "example": "18:00"
                    },
                    "end_time": {
                        "type": "string",
                        "format": "time",
                        "example": "19:00"
                    },
                    "day": {
                        "type": "string",
                        "format": "date",
                        "example": "2025-06-17"
                    }
                },
                "type": "object"
            },
            "TeamAvailabilityRequest": {
                "required": [
                    "availability"
                ],
                "properties": {
                    "availability": {
                        "type": "array",
                        "items": {
                            "required": [
                                "day",
                                "start_time",
                                "end_time"
                            ],
                            "properties": {
                                "day": {
                                    "type": "string",
                                    "enum": [
                                        "Monday",
                                        "Tuesday",
                                        "Wednesday",
                                        "Thursday",
                                        "Friday",
                                        "Saturday",
                                        "Sunday"
                                    ]
                                },
                                "start_time": {
                                    "type": "string",
                                    "format": "time",
                                    "example": "09:00"
                                },
                                "end_time": {
                                    "type": "string",
                                    "format": "time",
                                    "example": "17:00"
                                }
                            },
                            "type": "object"
                        }
                    }
                },
                "type": "object"
            },
            "TeamRequest": {
                "required": [
                    "name"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 255,
                        "example": "Development Team"
                    }
                },
                "type": "object"
            }
        }
    },
